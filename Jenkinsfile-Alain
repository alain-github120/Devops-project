Pipeline{
    agent any
    tools{
        maven 'maven3.96'
    }
    stages{
        stage('Git Checkout'){
            steps{
                git url:'https://github.com/alain-github120/Devops-project.git',branch:'nel'

            }
        }
         stage('Build maven application'){
            steps{
                sh 'mvn clean install'

            }
        }


    }

    
}